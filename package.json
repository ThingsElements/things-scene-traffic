{
  "name": "@things-elements/things-scene-traffic",
  "version": "0.1.0",
  "description": "An traffic monitor component for things-scene.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "serve": "watchify src/*.js -o things-scene-traffic.js src/index.js & polymer serve",
    "test": "./node_modules/.bin/mocha --require should --compilers js:babel-core/register test/**/test-*.js",
    "build": "browserify src/index.js -o things-scene-traffic.js && uglifyjs things-scene-traffic.js -o things-scene-traffic-min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThingsElements/things-scene-traffic.git"
  },
  "author": "",
  "license": "http://things-scene.hatiolab.com/EULA/",
  "bugs": {
    "url": "https://github.com/ThingsElements/things-scene-traffic/issues"
  },
  "homepage": "https://github.com/ThingsElements/things-scene-traffic#readme",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "~7.2.0",
    "browserify": "~10.2.4",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "should": "^8.0.2",
    "uglify-js": "^3.0.17",
    "watchify": "^3.3.1",
    "node-canvas": "Automattic/node-canvas",
    "gulp": "^3.8.5",
    "gulp-header-license": "^1.0.9"
  },
  "dependencies": {}
}
